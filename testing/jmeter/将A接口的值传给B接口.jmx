      <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="签名" enabled="true">
        <stringProp name="filename"></stringProp>
        <stringProp name="parameters"></stringProp>
        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="script">import org.apache.jmeter.config.Argument;
import org.apache.jmeter.config.Arguments;
import com.jmeter.plugin.SignTool;
import org.apache.jmeter.protocol.http.control.Header;
var params = new TreeMap();
var headers = new HashMap();
vars.put(&quot;deviceId&quot;, SignTool.randInt().toString());
params.put(&quot;secret&quot;, &quot;后台定义的值&quot;);
sampler.getHeaderManager().removeHeaderNamed(&quot;sign&quot;);
for (int i = 0; i &lt; sampler.getHeaderManager().getHeaders().size(); i++) {
	var header = sampler.getHeaderManager().getHeader(i);
	if (header.getName() == null || header.getName().length() == 0 || header.getValue() == null || header.getValue().length() == 0) {
		continue;
	}
	log.info(&quot;请求头：&quot; + header.getName() + &quot; : &quot; + header.getValue());
	headers.put(header.getName(), header.getValue());
}
Arguments arguments = sampler.getArguments();
for (int i = 0; i &lt; arguments.getArgumentCount(); i++) {
	Argument argument = arguments.getArgument(i);
	String name = argument.getName();
	String value = argument.getValue();
	if (name == null || name.length() == 0 || value == null || value.length() == 0) {
		continue;
	}
	log.info(name + &quot;:&quot; + value);
	params.put(name, value);
}
log.info(&quot;签名前的字符串：&quot; + SignTool.getSignString(params, headers));
var sign = SignTool.sign(params, headers);
sampler.getHeaderManager().add(new Header(&quot;sign&quot;, sign));
log.info(&quot;签名后的字符串：&quot; + sign);</stringProp>
      </BeanShellPreProcessor>
